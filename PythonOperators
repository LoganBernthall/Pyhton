from tabulate import tabulate

#create data
data = [["+", "Addition" , "x+y"], 
        ["-", "Subtraction" , "x-y"], 
        ["*", "Multiplication" , "x*y"], 
        ["/", "Division" , "x/y"], 
        ["%", "Modulus" , "x%y"], 
        ["**", "Exponentiation" , "x**y"], 
        ["//", "Floor division" , "x//y"]]
  
#define header names
col_names = ["Operator", "Name" , "Example"]
  
#display table
print(tabulate(data, headers=col_names))
